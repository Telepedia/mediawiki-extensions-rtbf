<div class="rtbf-request-information">
    {{#req}}
        <div class="rtbf-wrapper">
            <div class="rtbf-left">
                <ul>
                    <li>
                        <b>Original Username</b>: <i>{{originalUsername}}</i>
                    </li>
                    <li>
                        <b>Target Username</b>: <i>{{targetUsername}}</i>
                    </li>
                </ul>
            </div>
            <div class="rtbf-right">
                <ul>
                    <li>
                        <b>Requested</b>: <i>{{requestedAt}}</i>
                    </li>
                    <li class="flex-notice">
                        <b>Status:</b> {{{statusNotice}}}
                    </li>
                </ul>
            </div>
        </div>
    {{/req}}
</div>
<h2>Anonymisation on Wikis</h2>
<div class="cdx-table">
    <div class="cdx-table__header">
        <div class="cdx-table__header__caption" aria-hidden="true">
            {{ title }}
        </div>
        <div class="cdx-table__header__header-content"></div>
    </div>
    <div class="cdx-table__table-wrapper">
        <table class="cdx-table__table">
            <caption>
                {{ title }}
            </caption>
            <thead>
            <tr>
                <th scope="col">
                    <span class="cdx-table__th-content">Wiki ID</span>
                </th>
                <th scope="col">
                    <span class="cdx-table__th-content">Status</span>
                </th>
                <th scope="col">
                    <span class="cdx-table__th-content">Error</span>
                </th>
                <th scope="col" class="cdx-table__table__cell--align-number">
                    <span class="cdx-table__th-content">Updated At</span>
                </th>
            </tr>
            </thead>
            <tbody>
            {{#jobs}}
                <tr>
                    <td>{{wiki_id}}</td>
                    <td>{{{status}}}</td>
                    <td>{{error_message}}</td>
                    <td class="cdx-table__table__cell--align-number">{{updated_at}}</td>
                </tr>
            {{/jobs}}
            </tbody>
        </table>
    </div>
</div>